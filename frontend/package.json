{

  "name": "frontend-monorepo",

  "private": true,

  "version": "0.0.0",

  "workspaces": [

    "packages/core",

    "packages/barberbooking",

    "packages/shared"

  ],

  "scripts": {

    "dev:core": "npm --workspace core run dev",

    "build:core": "npm --workspace core run build",

    "preview:core": "npm --workspace core run preview",

    "dev:booking": "npm --workspace booking run dev",

    "build:booking": "npm --workspace booking run build",

    "build:all": "npm run build:core && npm run build:booking"

  },

  "devDependencies": {

    "@types/jest": "^30.0.0",

    "@vitejs/plugin-react": "^4.5.0",

    "jest": "^29.7.0",

    "vite": "^6.3.5"

  },

  "dependencies": {

    "@heroicons/react": "^2.2.0",

    "date-fns": "^4.1.0",

    "flowbite": "^3.1.2",

    "flowbite-datepicker": "^1.3.2",

    "framer-motion": "^12.16.0",

    "gsap": "^3.13.0",

    "lucide-react": "^0.525.0",

    "react": "^19.1.0",

    "react-big-calendar": "^1.19.4",

    "react-dom": "^19.1.0",

    "react-icons": "^5.5.0",

    "recharts": "^3.0.0",

    "tailwindcss-filters": "^3.0.0"

  }

}

