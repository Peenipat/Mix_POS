{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@object/shared": ["packages/shared/src/index.ts"],
      "@object/shared/*": ["packages/shared/src/*"]
    }
  },
  "include": ["packages/*/src"]
  
}
